        <attr name="tpv_markerTextSize" format="dimension" />

        <attr name="tpv_markerColor" format="color" />
        <attr name="tpv_markerTextColor" format="color" />

        <attr name="tpv_showMarkers" format="boolean" />
        <attr name="tpv_showMarkerText" format="boolean" />

        <attr name="tpv_showCenterPoint" format="boolean" />
